{
    "name": "welltonmiranda/correios",
    "description": "Package para pesquisa de serviços de busca por cep, cálculo de frete e etc, diretamente nos serviços dos Correios.",
    "keywords": ["cep", "brazil", "laravel", "frete"],
    "license": "MIT",
    "authors": [{
        "name": "Wellton J. Miranda",
        "email": "welltonmiranda@gmail.com"
    }],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": ">=6.3",
        "laravel/framework": "^5.0||^6.0||^7.0"
    },
    "autoload": {
        "psr-0": {
            "WelltonMiranda\\Correios": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "WelltonMiranda\\Correios\\ServiceProvider"
            ],
            "aliases": {
                "Correios": "WelltonMiranda\\Correios\\Facade"
            }
        }
    }
}